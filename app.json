{
  "expo": {
    "name": "Goods4Goods",
    "slug": "goods4goods",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goods4goods.app"
    },
    "android": {
      "package": "com.goods4goods.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-image-picker"]
  }
}
